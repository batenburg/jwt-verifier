{
    "name": "batenburg/jwt-verifier",
    "description": "Verify JWT for Symfony and Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Batenburg",
            "email": "p.batenburg@live.nl"
        }
    ],
    "require": {
        "php": ">= 7.4.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.0 || ^7.1 || ^7.2",
        "symfony/framework-bundle": "^4.4 || ^5.1 || ^5.2"
    },
    "require-dev": {
        "lcobucci/jwt": "^3.4",
        "php-coveralls/php-coveralls": "^2.4",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^4.3",
        "web-token/jwt-framework": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Batenburg\\JWTVerifier\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Batenburg\\JWTVerifier\\Test\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
